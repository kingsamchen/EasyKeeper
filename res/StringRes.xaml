<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:EasyKeeper"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <sys:String x:Key="ChooseLocation">Choose a Location...</sys:String>
    <sys:String x:Key="NoVaultSelected">Please select an existing vault</sys:String>
    <sys:String x:Key="VaultNotFound">
        The vault is not found on your disk. Remove it from history?
    </sys:String>
    <sys:String x:Key="NoPasswordGiven">Please enter your password!</sys:String>
    <sys:String x:Key="AddAccountTitle">EasyKeeper - Add a New Account</sys:String>
    <sys:String x:Key="ModifyAccountTitle">EasyKeeper - Modify an Existing Account</sys:String>
    <sys:String x:Key="AccountInfoIncomplete">Please provide your account information!</sys:String>
    <sys:String x:Key="TagAlreadyExists">Account with the given tag already exists!</sys:String>
    <sys:String x:Key="ConfirmDeleteAccount">Are you sure to remove this record?</sys:String>
</ResourceDictionary>