<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:EasyKeeper"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <sys:String x:Key="ChooseLocation">Choose a Location...</sys:String>
    <sys:String x:Key="NoVaultSelected">Please select an existing vault</sys:String>
    <sys:String x:Key="VaultNotFound">
        The vault is not found on your disk. Remove it from history?
    </sys:String>
</ResourceDictionary>